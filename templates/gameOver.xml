<fest:template xmlns:fest="http://fest.mail.ru"
  context_name="json">
	<h1 class="window__headers">You win!</h1>
	<table>
		<tr>
			<td><h2 class="window__headers">Your score:</h2></td>
			<td><h2 id="newScore" class="window__headers"></h2></td>
		</tr>
		<tr>
			<td><h2 class="window__headers">Nickname:</h2> </td>
			<td><input id="nickname" class="window__input" type="text" /></td>
		</tr>
	</table>
	<ul class="buttons">
		<li class="buttons__left"><a id="save" href="javascript:void(0)" class="window__button">Save</a></li>
		<li class="buttons__right"><a id="dontsave" href="javascript:void(0)" class="window__button">Back</a></li>
	</ul>
</fest:template>